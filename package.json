{
    "private": true,
    "workspaces": {
        "packages": [
            "packages/*",
            "plugins/*",
            "themes/*"
        ]
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "tailwind": "npx tailwindcss -o resources/assets/sass/app.scss -i resources/assets/sass/app-input.scss --watch"
    },
    "devDependencies": {
        "axios": "^0.21",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.61.0",
        "sass-loader": "^12.1.0",
        "tailwindcss": "^3.4.10"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.24.3",
        "@google/generative-ai": "^0.15.0",
        "axios": "^1.6.7",
        "clipboard": "^2.0.11",
        "cropperjs": "^1.6.1",
        "epic-spinners": "^2.0.0",
        "jquery": "^3.7.1",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "openai": "^4.52.7",
        "sanitize-html": "^2.12.1",
        "tiny-emitter": "^2.1.0",
        "vue": "^3.3.4"
    }
}
